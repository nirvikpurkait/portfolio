{
  "$schema": "https://json.schemastore.org/package.json",
  "name": "portfolio-next",
  "version": "0.1.0",
  "private": true,
  "prisma": {
    "schema": "./src/database/prisma/schema.prisma"
  },
  "scripts": {
    "dev": "next dev --turbo",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "build-start": "npx run-s prisma-build build start",
    "postinstall": "npx run-s prisma-build prisma-generate",
    "prisma-build": "npx prisma migrate deploy",
    "prisma-dev": "npx prisma migrate dev",
    "prisma-generate": "npx prisma generate",
    "prisma-reset": "npx prisma migrate reset",
    "prisma-studio": "npx prisma studio",
    "prisma-push": "npx prisma db push",
    "prisma-format": "npx prisma format",
    "delete-dll": "cd \"node_modules/.prisma/client\" && rm \"query_engine-windows.dll.node\"",
    "check-update": "ncu",
    "install-update": "ncu -u && ncu install"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^6.5.1",
    "@fortawesome/free-brands-svg-icons": "^6.5.1",
    "@fortawesome/free-regular-svg-icons": "^6.5.1",
    "@fortawesome/free-solid-svg-icons": "^6.5.1",
    "@fortawesome/react-fontawesome": "^0.2.0",
    "@prisma/client": "^5.12.1",
    "@radix-ui/react-dialog": "^1.0.5",
    "@radix-ui/react-dropdown-menu": "^2.0.6",
    "@radix-ui/react-icons": "^1.3.0",
    "@radix-ui/react-slot": "^1.0.2",
    "@radix-ui/react-switch": "^1.0.3",
    "@react-pdf/renderer": "^3.3.8",
    "@tailwindcss/container-queries": "^0.1.1",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.0",
    "lucide-react": "^0.323.0",
    "mailchecker": "^6.0.1",
    "next": "^14.1.0",
    "next-auth": "^5.0.0-beta.9",
    "next-themes": "^0.2.1",
    "react": "^18",
    "react-dom": "^18",
    "react-hook-form": "^7.50.1",
    "react-hot-toast": "^2.4.1",
    "sass": "^1.70.0",
    "sharp": "^0.33.2",
    "tailwind-merge": "^2.2.1",
    "tailwindcss-animate": "^1.0.7",
    "uid": "^2.0.2",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@hookform/devtools": "^4.3.1",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "autoprefixer": "^10.4.17",
    "eslint": "^8",
    "eslint-config-next": "14.1.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^8",
    "prettier": "^3.2.5",
    "prettier-plugin-tailwindcss": "^0.5.11",
    "prisma": "^5.12.1",
    "tailwindcss": "^3.4.1",
    "typescript": "^5"
  }
}
